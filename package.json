{
  "name": "portfolio-v5",
  "type": "module",
  "scripts": {
    "dev:site": "astro dev",
    "dev:cv": "bun cv/watch.ts",
    "dev:i18n": "bun i18n/cli.ts generate --watch",
    "build": "bun check && bun build:i18n && bun build:cv && bun build:site",
    "build:site": "astro build",
    "build:cv": "bun cv/generate.tsx",
    "build:i18n": "bun i18n/cli.ts generate",
    "check": "bun check:i18n && bun check:biome && bun check:astro",
    "check:astro": "astro check",
    "check:i18n": "bun i18n/cli.ts check",
    "check:biome": "biome check --apply {cv,i18n,locales} *.json"
  },
  "dependencies": {
    "@astrojs/check": "0.4.1",
    "@astrojs/tailwind": "5.1.0",
    "@biomejs/biome": "1.5.3",
    "@iconify/json": "2.2.172",
    "@iconify/tailwind": "0.1.4",
    "@react-pdf/renderer": "3.3.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "18.2.48",
    "@types/react-pdf": "7.0.0",
    "astro": "4.2.1",
    "bun-types": "1.0.25",
    "commander": "11.1.0",
    "date-fns": "3.3.1",
    "dts-dom": "3.7.0",
    "nanostores": "0.9.5",
    "react": "18.2.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  }
}
